{
  "name": "valorantinfo",
  "version": "1.0.0",
  "description": "Consuming valorant API",
  "source": "./src/index.html",
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "build": "rm -rf dist/* && parcel build src/index.html --public-url . && npm run copy",
    "format": "prettier --write \"src/**/*.{js,jsx}\"",
    "lint": "eslint \"src/**/*.{js,jsx}\" --quiet",
    "dev": "parcel src/index.html",
    "copy": "cp _redirects dist/"
  },
  "keywords": [
    "Valorant",
    "API",
    "Games"
  ],
  "author": "Alexander Tzoc <jealtzo26@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/eslint-parser": "^7.16.3",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "gh-pages": "^3.2.3",
    "parcel": "^2.0.1",
    "prettier": "^2.5.1"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router": "^6.1.1",
    "react-router-dom": "^6.1.1"
  },
  "browserslist": [
    "last 2 Chrome versions"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Achess01/Valorant-Info.git"
  },
  "bugs": {
    "url": "https://github.com/Achess01/Valorant-Info/issues"
  },
  "homepage": "."
}
